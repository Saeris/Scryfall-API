{
  "name": "scryfall-api",
  "version": "1.0.0",
  "author": "Drake Costa <drake@saeris.io> (http://saeris.github.io/)",
  "description": "A GraphQL API wrapper for Scryfall built with Apollo Datasources.",
  "private": true,
  "license": "MIT",
  "keywords": [
    "scryfall",
    "magic the gathering",
    "apollo server",
    "graphql",
    "mtg"
  ],
  "main": "src/index.ts",
  "scripts": {
    "start": "serverless offline start --dontPrintOutput",
    "deploy": "yarn deploy:dev",
    "deploy:dev": "cross-env SLS_DEBUG=TRUE serverless --stage=dev deploy",
    "deploy:prod": "serverless --stage=production deploy",
    "debug": "set SLS_DEBUG=* && yarn start",
    "lint": "eslint ./src",
    "test": "yarn lint && jest -c ./jest.config.js",
    "test:watch": "jest -c ./jest.config.js --watch",
    "test:coverage": "jest -c ./jest.config.js && codecov",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@saeris/graphql-directives": "^0.3.0",
    "@saeris/graphql-scalars": "^1.4.0",
    "apollo-datasource": "^0.7.0",
    "apollo-datasource-rest": "^0.8.0",
    "apollo-server-lambda": "^2.11.0",
    "aws-lambda": "^1.0.5",
    "datauri": "^2.0.0",
    "date-fns": "^2.11.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.7",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "node-vibrant": "^3.1.5",
    "potrace": "^2.1.2",
    "querystring": "^0.2.0",
    "regenerator-runtime": "^0.13.5",
    "svgo": "^1.3.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/aws-lambda": "^8.10.46",
    "@types/graphql": "^14.5.0",
    "@types/node": "^13.9.1",
    "@types/svgo": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "aws-sdk": "^2.640.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import-graphql": "^2.7.0",
    "codecov": "^3.6.5",
    "core-js": "3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-promise": "^4.2.1",
    "graphql-cli": "^3.0.14",
    "jest": "^25.1.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "serverless": "^1.66.0",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.42.0",
    "webpack-node-externals": "^1.6.0"
  }
}
